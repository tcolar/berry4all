TIMEOUT 10
ABORT 'BUSY'
ABORT 'NO ANSWER'
ABORT 'ERROR'
SAY 'Starting GPRS connect script\n'

""'AT'
OK 'ATE0V1'
OK 'AT'
OK 'AT+CGDCONT=1,"IP","internet2.voicestream.com"'
ABORT 'NO CARRIER'
OK 'ATS0=0'
OK 'BBT_OS'
OK 'AT'
OK 'ATE0V1'
OK 'AT+CGDCONT=1,"IP","internet2.voicestream.com"'
ABORT 'NO CARRIER'
SAY 'Dialing...\n'
OK 'ATD*99#'
CONNECT
~p
