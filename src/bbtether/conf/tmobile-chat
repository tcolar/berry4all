TIMEOUT 10
ABORT 'BUSY'
ABORT 'NO ANSWER'
ABORT 'ERROR'
ABORT "NO DIALTONE"
ABORT VOICE
ABORT RINGING

SAY 'Starting GPRS connect script\n'

""'AT'
SAY 'Setting APN\n'
OK 'ATE0V1'
OK 'AT'
OK 'AT+CGDCONT=1,"IP","wap.voicestream.com"'
ABORT 'NO CARRIER'
OK 'ATS0=0'
OK 'ATE0V1'
OK 'AT'
OK 'AT+CGDCONT=1,"IP","wap.voicestream.com"'
ABORT 'NO CARRIER'
SAY 'Dialing...\n'
OK 'ATD*99**1#'
CONNECT
~p
